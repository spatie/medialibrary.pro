{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.158",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.9",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/commonmark": "^1.4",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-flash": "^1.7",
        "spatie/laravel-honeypot": "^3.0",
        "spatie/laravel-medialibrary": "v9.x-dev",
        "spatie/laravel-medialibrary-pro": "dev-master",
        "spatie/laravel-menu": "^3.6",
        "spatie/laravel-stubs": "^2.0",
        "spatie/laravel-tail": "^4.3",
        "spatie/laravel-webhook-client": "^2.7",
        "spatie/temporary-directory": "^1.2",
        "spatie/test-time": "^1.1",
        "spatie/yaml-front-matter": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "fzaninotto/faker": "^1.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-medialibrary"
        },
        {
            "type": "path",
            "url": "../laravel-medialibrary-pro"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
