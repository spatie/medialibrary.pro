{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.mailcoach.app"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.158",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.2",
        "fzaninotto/faker": "^1.4",
        "laravel/framework": "^7.28",
        "laravel/horizon": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "spatie/commonmark-highlighter": "^1.0",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-demo-mode": "^2.7",
        "spatie/laravel-flash": "^1.7",
        "spatie/laravel-honeypot": "^2.3",
        "spatie/laravel-menu": "^3.6",
        "spatie/laravel-tail": "^4.3",
        "spatie/laravel-webhook-client": "^2.7",
        "spatie/temporary-directory": "^1.2",
        "spatie/test-time": "^1.1",
        "spatie/yaml-front-matter": "^2.0",
        "vimeo/laravel": "^5.5",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "fzaninotto/faker": "^1.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag mailcoach-assets --force"
        ]
    }
}
